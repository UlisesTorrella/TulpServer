<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>TulpServer</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpServerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpServer</servlet-name>
		<url-pattern>/tulpserver</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Session</servlet-name>
		<servlet-class>com.ulises.tulpserver.SessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Session</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TulpNotificaciones</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpNotificacionesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpNotificaciones</servlet-name>
		<url-pattern>/tulpnotificaciones</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TulpAllUsers</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpAllUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpAllUsers</servlet-name>
		<url-pattern>/tulpallusers</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TulpAddUser</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpAddUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpAddUser</servlet-name>
		<url-pattern>/tulpadduser</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>TulpResetDailyPoints</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpResetDailyPointsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpResetDailyPoints</servlet-name>
		<url-pattern>/tulpresetdaily</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TulpSearch</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpSearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpSearch</servlet-name>
		<url-pattern>/tulpsearch</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TulpFriends</servlet-name>
		<servlet-class>com.ulises.tulpserver.TulpFriendsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TulpFriends</servlet-name>
		<url-pattern>/tulpfriends</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<security-constraint>
        <web-resource-collection>
           <web-resource-name>TulpResetDailyPoints</web-resource-name>
            <url-pattern>/tulpresetdaily/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>
    
    <security-constraint>
        <web-resource-collection>
            <url-pattern>/</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>*</role-name>
        </auth-constraint>
    </security-constraint>
    

 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value>com.ulises.tulpserver.TulpServerServlet</param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>